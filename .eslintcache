ents\\FaqSection.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\components\\GlobalStyle.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\styles.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\components\\Nav.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\pages\\ContactUs.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\pages\\OurWork.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\pages\\MovieDetail.js",["74"],"import React, { useState, useEffect } from \"react\";\r\nimport styled from \"styled-components\";\r\nimport { useHistory } from \"react-router-dom\";\r\nimport { MovieState } from \"../movieState.js\";\r\n// Animations\r\nimport { motion } from \"framer-motion\";\r\nimport { pageAnimation } from \"../animation.js\";\r\n\r\nconst MovieDetail = () => {\r\n  const history = useHistory();\r\n  const url = history.location.pathname;\r\n  const [movies, setMovies] = useState(MovieState);\r\n  const [movie, setMovie] = useState(null);\r\n\r\n  // useEffect\r\n  useEffect(() => {\r\n    const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\r\n    setMovie(currentMovie[0]);\r\n  }, [movies, url]);\r\n  return (\r\n    <>\r\n      {movie && (\r\n        <Details\r\n          variants={pageAnimation}\r\n          initial=\"hidden\"\r\n          animate=\"show\"\r\n          exit=\"exit\"\r\n        >\r\n          <HeadLine>\r\n            <h2>{movie.title}</h2>\r\n            <img src={movie.mainImg} alt=\"Movie\" />\r\n          </HeadLine>\r\n          <Awards>\r\n            {movie.awards.map((award) => (\r\n              <Award\r\n                title={award.title}\r\n                description={award.description}\r\n                key={award.title}\r\n              />\r\n            ))}\r\n          </Awards>\r\n          <ImageDisplay>\r\n            <img src={movie.secondaryImg} alt=\"Secondary movie\" />\r\n          </ImageDisplay>\r\n        </Details>\r\n      )}\r\n    </>\r\n  );\r\n};\r\n\r\nconst Details = styled(motion.div)`\r\n  color: white;\r\n`;\r\n\r\nconst HeadLine = styled.div`\r\n  min-height: 90vh;\r\n  padding-top: 20vh;\r\n  position: relative;\r\n  h2 {\r\n    position: absolute;\r\n    top: 10%;\r\n    left: 50%;\r\n    transform: translate(-50%, -10%);\r\n  }\r\n  img {\r\n    width: 100%;\r\n    height: 70vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\nconst Awards = styled.div`\r\n  min-height: 80vh;\r\n  display: flex;\r\n  margin: 5rem 10rem;\r\n  align-items: center;\r\n  justify-content: space-around;\r\n`;\r\n\r\nconst AwardStyle = styled.div`\r\n  padding: 5rem;\r\n  h3 {\r\n    font-size: 2rem;\r\n  }\r\n  .line {\r\n    width: 100%;\r\n    background: #23d997;\r\n    height: 0.5rem;\r\n    margin: 1rem 0rem;\r\n  }\r\n  p {\r\n    padding: 2rem 0rem;\r\n  }\r\n`;\r\n\r\nconst ImageDisplay = styled.div`\r\n  min-height: 50vh;\r\n  img {\r\n    width: 100%;\r\n    height: 100vh;\r\n    object-fit: cover;\r\n  }\r\n`;\r\n\r\n// Award component\r\nconst Award = ({ title, description }) => {\r\n  return (\r\n    <AwardStyle>\r\n      <h3>{title}</h3>\r\n      <div className=\"line\"></div>\r\n      <p>{description}</p>\r\n    </AwardStyle>\r\n  );\r\n};\r\n\r\nexport default MovieDetail;\r\n","C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\movieState.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\animation.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\components\\Wave.js",[],"C:\\Users\\dorfo\\Desktop\\The Creative React and Redux Course\\capture\\src\\components\\Toggle.js",[],{"ruleId":"75","replacedBy":"76"},{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","severity":1,"message":"80","line":12,"column":18,"nodeType":"81","messageId":"